import React from 'react';

export default function NavigationMenu({ links }) {
  return (
    <nav className="flex items-center gap-6">
      {links.map((link, i) => (
        <a key={i} href={link.href} className="text-sm font-medium text-gray-400 hover:text-violet-400 transition-colors">
          {link.label}
        </a>
      ))}
    </nav>
  );
}